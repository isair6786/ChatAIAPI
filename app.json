{
    "name": "assistantui",
    "slug": "assistantui",
    "scheme": "assistantui",
    "splash": {
        "backgroundColor": "#ffffff"
    },
    "plugins": [
        [
            "expo-calendar",
            {
                "calendarPermission": "The app needs to access your calendar."
            }
        ]
    ],
    "expo": {
        "scheme": "assistantui",
        "splash": {
            "backgroundColor": "#ffffff"
        },
        "extra": {
            "eas": {
                "projectId": "d9995a21-062f-488f-ae0d-9e5d06cd1aee"
            }
        },
        "android": {
            "package": "com.AssistantUI.AssistantAI",
            "android": {
                "intentFilters": [{
                    "action": "VIEW",
                    "data": [{
                        "scheme": "assistantui"
                    }],
                    "category": ["BROWSABLE", "DEFAULT"]
                }]
            },
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            }

        }
    }
}